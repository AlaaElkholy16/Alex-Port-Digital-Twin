# Copy this file to .env and fill with your MyShipTracking credentials
MYSHIPTRACKING_API_KEY=YOUR_API_KEY_HERE
MYSHIPTRACKING_SECRET=YOUR_SECRET_HERE
MYSHIPTRACKING_PORT=Alexandria
# Default endpoint for the Vessels In Port API. Adjust if MyShipTracking updates the path.
MYSHIPTRACKING_ENDPOINT=https://api.myshiptracking.com/v1/ports/vessels
# query (default) appends key+secret to the URL. Set to 'header' if your plan supports auth headers.
MYSHIPTRACKING_AUTH_MODE=query
